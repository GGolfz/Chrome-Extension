{
    "manifest_version": 2,
    "name": "Youtube Adblocker",
    "description": "Youtube AdBlocker Extension by GGolfz",
    "version": "1.0.0",
    "browser_action": {
        "default_icon": "logo.png"
    },
    "background": {
        "scripts": ["script.js"],
        "persistent": true
    },
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "icons": {"128":"logo.png"}
}